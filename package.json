{
  "name": "portfolio-sv",
  "version": "2.0.51",
  "type": "module",
  "license": "MIT",
  "author": "Leomotors",
  "description": "My Portfolio written in Svelte",
  "main": "index.js",
  "repository": "https://github.com/Leomotors/portfolio-sv",
  "scripts": {
    "dev": "yarn lm-inc && yarn lm-config && svelte-kit dev",
    "build": "yarn lm-config && svelte-kit build",
    "package": "svelte-kit package",
    "preview": "svelte-kit preview",
    "check": "svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-check --tsconfig ./tsconfig.json --watch",
    "lint": "prettier --check \"src/**/*.{ts,svelte}\"",
    "format": "prettier --write \"src/**/*.{ts,svelte}\""
  },
  "devDependencies": {
    "@sveltejs/adapter-vercel": "next",
    "@sveltejs/kit": "next",
    "@typescript-eslint/eslint-plugin": "^5.11.0",
    "@typescript-eslint/parser": "^5.11.0",
    "autoprefixer": "^10.4.2",
    "cssnano": "^5.0.17",
    "eslint": "^8.8.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-svelte3": "^3.4.0",
    "leomotors-automation": "^3.0.0",
    "postcss": "^8.4.6",
    "postcss-load-config": "^3.1.1",
    "prettier": "^2.5.1",
    "prettier-plugin-svelte": "^2.6.0",
    "svelte": "^3.46.4",
    "svelte-check": "^2.4.3",
    "svelte-preprocess": "^4.10.2",
    "tailwindcss": "^3.0.19",
    "tslib": "^2.3.1",
    "typescript": "^4.5.5"
  },
  "dependencies": {
    "vanta": "^0.5.21"
  }
}