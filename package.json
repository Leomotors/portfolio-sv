{
  "name": "portfolio-sv",
  "version": "2.0.35",
  "scripts": {
    "dev": "yarn lm-inc && yarn lm-config && svelte-kit dev",
    "build": "yarn lm-config && svelte-kit build",
    "package": "svelte-kit package",
    "preview": "svelte-kit preview",
    "check": "svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-check --tsconfig ./tsconfig.json --watch",
    "lint": "prettier --ignore-path .gitignore --check --plugin-search-dir=. . && eslint --ignore-path .gitignore .",
    "format": "prettier --ignore-path .gitignore --write --plugin-search-dir=. ."
  },
  "devDependencies": {
    "@sveltejs/adapter-auto": "next",
    "@sveltejs/kit": "next",
    "@typescript-eslint/eslint-plugin": "^5.9.1",
    "@typescript-eslint/parser": "^5.9.1",
    "eslint": "^8.6.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-svelte3": "^3.4.0",
    "leomotors-automation": "^2.0.1",
    "prettier": "^2.5.1",
    "prettier-plugin-svelte": "^2.6.0",
    "svelte": "^3.46.2",
    "svelte-check": "^2.3.0",
    "svelte-preprocess": "^4.10.1",
    "tslib": "^2.3.1",
    "typescript": "^4.5.4",
    "postcss": "^8.4.5",
    "postcss-load-config": "^3.1.1",
    "autoprefixer": "^10.4.2",
    "cssnano": "^5.0.15",
    "tailwindcss": "^3.0.15"
  },
  "type": "module",
  "license": "MIT",
  "author": "Leomotors",
  "description": "My Portfolio written in Svelte",
  "main": "index.js",
  "repository": "https://github.com/Leomotors/portfolio-sv"
}