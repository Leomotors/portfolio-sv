{
  "name": "portfolio-sv",
  "version": "2.0.124",
  "type": "module",
  "license": "MIT",
  "author": "Leomotors",
  "description": "My Portfolio written in Svelte",
  "main": "index.js",
  "repository": "https://github.com/Leomotors/portfolio-sv",
  "scripts": {
    "dev": "yarn lm-inc && yarn lm-config && svelte-kit dev",
    "build": "yarn lm-config && svelte-kit build",
    "package": "svelte-kit package",
    "preview": "svelte-kit preview",
    "check": "svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-check --tsconfig ./tsconfig.json --watch",
    "lint": "prettier --check \"*.config.{cjs,js,ts}\" && prettier --check \"src/**/*.{ts,svelte}\" && eslint src && eslint *.{cjs,js}",
    "format": "prettier --write \"*.config.{cjs,js,ts}\" && prettier --write \"src/**/*.{ts,svelte}\""
  },
  "devDependencies": {
    "@leomotors/scripts": "^3.1.0",
    "@octokit/types": "^6.34.0",
    "@sveltejs/adapter-vercel": "1.0.0-next.47",
    "@sveltejs/kit": "1.0.0-next.306",
    "@tailwindcss/typography": "^0.5.2",
    "@types/marked": "^4.0.3",
    "@types/node-emoji": "^1.8.1",
    "@types/tailwindcss": "^3.0.10",
    "@typescript-eslint/eslint-plugin": "^5.18.0",
    "@typescript-eslint/parser": "^5.18.0",
    "autoprefixer": "^10.4.4",
    "chalk": "^5.0.1",
    "cssnano": "^5.1.7",
    "eslint": "^8.12.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-svelte3": "^3.4.1",
    "github-profile-status": "^1.0.0",
    "highlight.js": "^11.5.0",
    "marked": "^4.0.12",
    "node-emoji": "^1.11.0",
    "postcss": "^8.4.12",
    "postcss-load-config": "^3.1.4",
    "prettier": "^2.6.2",
    "prettier-plugin-svelte": "^2.6.0",
    "svelte": "^3.46.6",
    "svelte-check": "^2.4.6",
    "svelte-preprocess": "^4.10.5",
    "tailwindcss": "^3.0.23",
    "tslib": "^2.3.1",
    "typescript": "^4.6.3",
    "vanta": "^0.5.21",
    "vite": "^2.9.1",
    "vite-plugin-inspect": "^0.4.3"
  }
}