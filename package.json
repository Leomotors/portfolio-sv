{
  "name": "portfolio-sv",
  "version": "3.0.130",
  "type": "module",
  "license": "MIT",
  "author": "Leomotors",
  "description": "My Portfolio written in SvelteKit",
  "repository": "https://github.com/Leomotors/portfolio-sv",
  "scripts": {
    "dev": "yarn lm-inc && yarn lm-config && svelte-kit dev",
    "build": "yarn lm-config && svelte-kit build",
    "package": "svelte-kit package",
    "preview": "svelte-kit preview",
    "check": "svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-check --tsconfig ./tsconfig.json --watch",
    "lint": "prettier --check \"*.config.{cjs,js,ts}\" && prettier --check \"src/**/*.{ts,svelte}\" && eslint src && eslint *.{cjs,js}",
    "format": "prettier --write \"*.config.{cjs,js,ts}\" && prettier --write \"src/**/*.{ts,svelte}\""
  },
  "devDependencies": {
    "@leomotors/scripts": "^3.1.0",
    "@sveltejs/adapter-vercel": "1.0.0-next.56",
    "@sveltejs/kit": "1.0.0-next.345",
    "@types/tailwindcss": "^3.0.10",
    "@typescript-eslint/eslint-plugin": "^5.26.0",
    "@typescript-eslint/parser": "^5.26.0",
    "autoprefixer": "^10.4.7",
    "chalk": "^5.0.1",
    "cssnano": "^5.1.10",
    "eslint": "^8.16.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-svelte3": "^4.0.0",
    "postcss": "^8.4.14",
    "postcss-load-config": "^4.0.0",
    "prettier": "^2.6.2",
    "prettier-plugin-svelte": "^2.7.0",
    "svelte": "^3.48.0",
    "svelte-check": "^2.7.1",
    "svelte-preprocess": "^4.10.6",
    "tailwindcss": "^3.0.24",
    "tslib": "^2.4.0",
    "typescript": "^4.7.2",
    "vite": "^2.9.9"
  }
}